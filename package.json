{
  "name": "dotio2",
  "version": "0.0.0",
  "description": "Static site for vixalien",
  "private": true,
  "scripts": {
    "build": "explosiv build",
    "start": "explosiv serve",
    "dev": "explosiv dev",
    "prebuild": "yarn build:js && yarn build:css && yarn build:optimize-images",
    "postbuild": "node src/feed.js && node src/wbn.js",
    "build:js": "node src/build-js",
    "build:optimize-images": "node src/optimize-images.mjs",
    "build:css": "yarn build:css-app && yarn build:css-code && yarn build:css-image-container",
    "build:css-app": "stylus stylus/app.styl -o public/css -c",
    "build:css-code": "stylus stylus/highlight/highlight.styl -o public/css -c",
    "build:css-image-container": "stylus stylus/image-container.styl -o public/css -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vixalien/dotio2.git"
  },
  "keywords": [
    "dotio",
    "vixalien",
    "static",
    "site"
  ],
  "author": "vixalien",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/vixalien/dotio2/issues"
  },
  "homepage": "https://github.com/vixalien/dotio2#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.5",
    "explosiv": "^2.5.1",
    "feed": "^4.2.2",
    "front-matter": "^4.0.2",
    "glob": "^7.1.6",
    "highlight.js": "^11.1.0",
    "marked": "^4.0.7",
    "sharp": "^0.30.6",
    "stylus": "^0.55.0",
    "wbn": "0.0.6"
  },
  "devDependencies": {
    "@types/sharp": "^0.30.4"
  }
}
