#carbonads * {
	margin: initial;
	padding: initial
}

#carbonads {
	font-family: inherit
}

#carbonads {
	display: flex;
	max-width: 330px;
	z-index: 100;
	border: 1px solid;
	padding: 0;
	margin-bottom: 20px
}

#carbonads a {
	color: inherit;
	text-decoration: none;
	border: 0
}

#carbonads a:hover {
	color: inherit
}

#carbonads span {
	position: relative;
	display: block;
	overflow: hidden
}

#carbonads .carbon-wrap {
	display: flex;
}

#carbonads .carbon-img {
	display: block;
	margin: 0;
	line-height: 1;
	padding: 10px 0 0 10px;
}

#carbonads .carbon-img img {
	display: block
}

#carbonads .carbon-text {
	font-size: 13px;
	padding: 0 0 0 10px;
	margin-bottom: 0;
	line-height: 1.5;
	text-align: left;
	align-self: flex-start;
	padding: 10px 10px 0 10px;
}

#carbonads .carbon-poweredby {
	display: block;
	padding: 6px 8px;
	text-transform: uppercase;
	letter-spacing: .5px;
	font-weight: 600;
	font-size: 8px;
	line-height: 1;
	bottom: 0;
	right: 0;
	text-align: end
}
